-- arithmetic operators
1 + 2
1 - 2
1 * 2
1 / 2

-- value definition
def x = 1

-- function definition
def f x = x + 1

-- lambda
\a b -> a + b

-- lambda application
(\a b -> a + b) 1 2

-- function application
f x

-- negation
-x

-- not
!x

-- let expression
let x = 1 in x + 1

-- let expression function definition
let f x = x + 1 in f 1

-- do expression
do
  x <- Just 1;
  y <- Just 2;
  return (x + y)
end 